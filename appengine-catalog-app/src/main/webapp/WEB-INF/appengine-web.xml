<?xml version="1.0" encoding="UTF-8"?>
<appengine-web-app xmlns="http://appengine.google.com/ns/1.0">
    <threadsafe>true</threadsafe>
    <use-google-connector-j>true</use-google-connector-j>
	<runtime>java8</runtime>
	<warmup-requests-enabled>false</warmup-requests-enabled>    
    <system-properties>
        <property name="java.util.logging.config.file" value="WEB-INF/logging.properties"/>
        <property name="appengine.api.urlfetch.defaultDeadline" value="10"/>
        
        <property name="keyring-name" value="poc-storage" />
        <property name="key-name" value="poc-mysql-gclouddemo" />
        <property name="mysql-pwd-bucketname" value="wombat-ecom-detritus" />
        <property name="mysql-pwd-objectname" value="mysql-gclouddemo.txt" />
    	<property name="cloudsql-local" value="jdbc:mysql://104.198.41.158/${database}?user=${user}&amp;password=" />
    	<property name="cloudsql-deployed" value="jdbc:google:mysql://${instance_connection_name}/${database}?user=${user}&amp;password=" />
    </system-properties>
</appengine-web-app>